<%- include('partials/header') -%>


<div class="container contact-form bg-dark">
    <div class="contact-image">
        <i class="fab fa-rocketchat text-light fa-5x pt-3"></i>
        <form action="">
            <h3 class="text-light">We love to hear your feedback</h3>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <input type="text" name="txtName" class="form-control" placeholder="Your Name" value="">
                    </div>
                    <div class="form-group">
                        <input type="email" name="txtEmail" class="form-control" placeholder="Your Email" value="">
                    </div>
                    <div class="form-group">
                        <input type="text" name="txtPhone" class="form-control" placeholder="Your Phone" value="">
                    </div>
                    <div class="form-group">
                        <input type="submit" name="btnSubmit" onclick="" class="btnContact btn" value="Send Message">
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <textarea name="textMsg"  class="form-control" style="width: 100%; height: 150px;" id="" cols="30" rows="10"></textarea>
                    </div>
                    
                </div>
            </div>
        </form>
    </div>
</div>



<%- include('partials/footer') -%>